@import "../../styles/index"

.control-panel
  position absolute
  z-index 100
  width 100%
  border-radius 3px
  background-color rgba(250, 250, 250, 0.6)

  &__listing
    background-color rgba(250, 250, 250, 0.8) !important
    transition all .2s ease

    &:hover
      cursor pointer
      background-color white !important

